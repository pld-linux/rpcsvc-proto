+#: rpcgen/rpc_main.c:1389
+#, c-format
+msgid "\"infile\" is required for template generation flags.\n"
+msgstr "\"infile\" 是樣版產生旗標所需的\n"
+
+#: rpcgen/rpc_main.c:1394
+#, c-format
+msgid "Cannot have more than one file generation flag!\n"
+msgstr "無法使用超過一個的檔案產生旗標!\n"
+
+#: rpcgen/rpc_main.c:1403
+#, c-format
+msgid "usage: %s infile\n"
+msgstr "用法: %s infile\n"
+
+#: rpcgen/rpc_main.c:1404
+#, c-format
+msgid ""
+"\t%s [-abkCLNTM][-Dname[=value]] [-i size] [-I [-K seconds]] [-Y path] "
+"infile\n"
+msgstr ""
+"\t%s [-abkCLNTM][-D名稱[=值]] [-i 大小] [-I [-K 秒數]] [-Y 路徑] 輸入檔案\n"
+
+#: rpcgen/rpc_main.c:1406
+#, c-format
+msgid "\t%s [-c | -h | -l | -m | -t | -Sc | -Ss | -Sm] [-o outfile] [infile]\n"
+msgstr ""
+"\t%s [-c | -h | -l | -m | -t | -Sc | -Ss | -Sm] [-o 輸出檔案] [輸入檔案]\n"
+
+#: rpcgen/rpc_main.c:1408
+#, c-format
+msgid "\t%s [-s nettype]* [-o outfile] [infile]\n"
+msgstr "\t%s [-s 網路型態]* [-o 輸出檔案] [輸入檔案]\n"
+
+#: rpcgen/rpc_main.c:1409
+#, c-format
+msgid "\t%s [-n netid]* [-o outfile] [infile]\n"
+msgstr "\t%s [-n 網路識別符號]* [-o 輸出檔案] [輸入檔案]\n"
+
+#: rpcgen/rpc_main.c:1417
+msgid "options:\n"
+msgstr "選項：\n"
+
+#: rpcgen/rpc_main.c:1418
+msgid "-a\t\tgenerate all files, including samples\n"
+msgstr "-a\t\t產生所有檔案，包含樣本\n"
+
+#: rpcgen/rpc_main.c:1419
+msgid "-b\t\tbackward compatibility mode (generates code for SunOS 4.1)\n"
+msgstr "-b\t\t回溯相容模式 (產生用於 SunOS 4.1 的編碼)\n"
+
+#: rpcgen/rpc_main.c:1420
+msgid "-c\t\tgenerate XDR routines\n"
+msgstr "-c\t\t產生 XDR 常式\n"
+
+#: rpcgen/rpc_main.c:1421
+msgid "-C\t\tANSI C mode\n"
+msgstr "-C\t\tANSI C 模式\n"
+
+#: rpcgen/rpc_main.c:1422
+msgid "-Dname[=value]\tdefine a symbol (same as #define)\n"
+msgstr "-D名稱[=值]\t定義一個符號 (如同 #define)\n"
+
+#: rpcgen/rpc_main.c:1423
+msgid "-h\t\tgenerate header file\n"
+msgstr "-h\t\t產生標頭檔\n"
+
+#: rpcgen/rpc_main.c:1424
+msgid "-i size\t\tsize at which to start generating inline code\n"
+msgstr "-i 大小\t\t啟始產生內聯編碼的大小\n"
+
+#: rpcgen/rpc_main.c:1425
+msgid "-I\t\tgenerate code for inetd support in server (for SunOS 4.1)\n"
+msgstr "-I\t\t產生在伺服器中支援的 inetd 編碼 (用於 SunOS 4.1)\n"
+
+#: rpcgen/rpc_main.c:1426
+msgid "-K seconds\tserver exits after K seconds of inactivity\n"
+msgstr "-K 秒\t不作用 K 秒之後離開伺服器\n"
+
+#: rpcgen/rpc_main.c:1427
+msgid "-l\t\tgenerate client side stubs\n"
+msgstr "-l\t\t產生客戶端殘餘\n"
+
+#: rpcgen/rpc_main.c:1428
+msgid "-L\t\tserver errors will be printed to syslog\n"
+msgstr "-L\t\t伺服器錯誤將被列印到 syslog\n"
+
+#: rpcgen/rpc_main.c:1429
+msgid "-m\t\tgenerate server side stubs\n"
+msgstr "-m\t\t產生伺服器端殘餘\n"
+
+#: rpcgen/rpc_main.c:1430
+msgid "-M\t\tgenerate MT-safe code\n"
+msgstr "-M\t\t產生 MT-safe 編碼\n"
+
+#: rpcgen/rpc_main.c:1431
+msgid "-n netid\tgenerate server code that supports named netid\n"
+msgstr "-n netid\t產生支援具名 netid 的伺服器編碼\n"
+
+#: rpcgen/rpc_main.c:1432
+msgid "-N\t\tsupports multiple arguments and call-by-value\n"
+msgstr "-N\t\t支援多重引數和傳值呼叫\n"
+
+#: rpcgen/rpc_main.c:1433
+msgid "-o outfile\tname of the output file\n"
+msgstr "-o 輸出檔\t輸出檔案的名稱\n"
+
+#: rpcgen/rpc_main.c:1434
+msgid "-s nettype\tgenerate server code that supports named nettype\n"
+msgstr "-s nettype\t產生支援具名 nettype 的伺服器編碼\n"
+
+#: rpcgen/rpc_main.c:1435
+msgid "-Sc\t\tgenerate sample client code that uses remote procedures\n"
+msgstr "-Sc\t\t產生使用遠端程序的範例客戶端編碼\n"
+
+#: rpcgen/rpc_main.c:1436
+msgid "-Ss\t\tgenerate sample server code that defines remote procedures\n"
+msgstr "-Ss\t\t產生定義遠端程序的範例伺服器編碼\n"
+
+#: rpcgen/rpc_main.c:1437
+msgid "-Sm \t\tgenerate makefile template \n"
+msgstr "-Sm\t\t產生 makefile 模板\n"
+
+#: rpcgen/rpc_main.c:1438
+msgid "-t\t\tgenerate RPC dispatch table\n"
+msgstr "-t\t\t產生 RPC 派送表\n"
+
+#: rpcgen/rpc_main.c:1439
+msgid "-T\t\tgenerate code to support RPC dispatch tables\n"
+msgstr "-T\t\t產生支援 RPC 派送表的編碼\n"
+
+#: rpcgen/rpc_main.c:1440
+msgid "-Y path\t\tdirectory name to find C preprocessor (cpp)\n"
+msgstr "-Y 路徑\t\t要尋找 C 前置處理器 (cpp) 的目錄名稱\n"
+
+#: rpcgen/rpc_main.c:1441
+msgid "-5\t\tSysVr4 compatibility mode\n"
+msgstr "-5\t\tSysVr4 相容模式\n"
+
+#: rpcgen/rpc_main.c:1442
+msgid "--help\t\tgive this help list\n"
+msgstr "--help\t\t提供此說明列表\n"
+
+#: rpcgen/rpc_main.c:1443
+msgid "--version\tprint program version\n"
+msgstr "--version\t輸出程式版本\n"
+
+#: rpcgen/rpc_scan.c:116
+msgid "constant or identifier expected"
+msgstr "應該要有常數或識別符號才對"
+
+#: rpcgen/rpc_scan.c:312
+msgid "illegal character in file: "
+msgstr "檔案中有不合法的字元: "
+
+#: rpcgen/rpc_scan.c:351 rpcgen/rpc_scan.c:377
+msgid "unterminated string constant"
+msgstr "沒有結尾的字串常數"
+
+#: rpcgen/rpc_scan.c:383
+msgid "empty char string"
+msgstr "空的字元字串"
+
+#: rpcgen/rpc_scan.c:525 rpcgen/rpc_scan.c:535
+msgid "preprocessor error"
+msgstr "前置處理器錯誤"
